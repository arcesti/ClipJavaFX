<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clipsjavafx.ClipsController">
    <children>
      <WebView fx:id="webView" prefHeight="248.0" prefWidth="640.0" />
        <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="colId" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="ID" />
                <TableColumn fx:id="colTitulo" maxWidth="1.7976931348623157E308" minWidth="1.0" prefWidth="-1.0" text="Titulo" />
                <TableColumn fx:id="colArtista" maxWidth="1.7976931348623157E308" minWidth="1.0" prefWidth="-1.0" text="Artista" />
                <TableColumn fx:id="colDuracao" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Duração" />
                <TableColumn fx:id="colData" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Data" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
</VBox>
